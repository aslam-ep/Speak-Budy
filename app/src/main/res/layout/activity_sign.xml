<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignActivity">
    <FrameLayout
        android:id="@+id/camera_preview2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <ImageView
        android:id="@+id/objectBackButton2"
        android:src="@drawable/ic_baseline_arrow_back_24"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:clickable="true"
        android:layout_margin="15dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Scan Signs"
        android:fontFamily="@font/amaranth"
        android:textSize="30dp"
        android:layout_centerHorizontal="true"
        android:layout_margin="15dp"
        android:textColor="#fff"/>
    <ImageView
        android:id="@+id/flashButton2"
        android:src="@drawable/ic_baseline_flash_off_24"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_margin="15dp"
        android:clickable="true"
        android:layout_alignParentEnd="true"/>
    <TextView
        android:id="@+id/objectsResult2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hi there"
        android:textSize="20dp"
        android:fontFamily="@font/amaranth"
        android:layout_margin="35dp"
        android:textColor="#B2FFFFFF"
        android:layout_alignParentBottom="true" />
    <ImageView
        android:id="@+id/soundButton2"
        android:src="@drawable/ic_baseline_volume_up_24"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="15dp"
        android:clickable="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"/>
</android.widget.RelativeLayout>